{
  "manifest_version": 3,
  "name": "Color Palette Chrome Extension",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["src/scripts/content.ts"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["src/assets/cursor-24.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
